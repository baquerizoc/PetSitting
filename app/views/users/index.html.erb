<h1>Manage Users</h1>
    <table id = "index-user">
      <tr>
        <th>Name</th>
        <th>Last Name</th>
        <th>Phone Number</th>
        <th>Address</th>
        <th>Date of Birth</th>
      </tr>
      <% @users.each do |user|  %>
      <tr>
      	<td><%= user.name %></td>
      	<td><%= user.lastname %></td>
      	<td><%= user.phone %></td>
      	<td><%= user.address %></td>
      	<td><%= user.date_of_birth.strftime('%F') %></td>

      	<td><%= link_to "Edit User", edit_user_path(user), class: 'button', id: 'user-index-button' %></td>
        <td><%= link_to "View User", user_path(user), class: 'button', id: 'user-index-button' %></td>
        <td><%= link_to "Delete User", user_path(user), method: :delete, data: { confirm: 'Are you sure you want to delete this User?' }, class: 'button', id: 'user-index-button' %></td>
      </tr>

      <%end%>